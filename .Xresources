URxvt*termName: rxvt-256color
URxvt*scrollBar: false
URxvt*saveLines: 1024
!URxvt*loginShell: true
!URxvt*urgentOnBell: true
!URxvt*visualBell: true

URxvt*perl-ext: default,url-select,vtwheel,font-size

URxvt*keysym.M-u: perl:url-select:select_next
URxvt*url-select.launcher: chromium
URxvt*url-select.underline: true

URxvt*iso14755: false
URxvt*iso14755_52: false
URxvt*keysym.C-Up: perl:font-size:increase
URxvt*keysym.C-Down: perl:font-size:decrease
URxvt*keysym.C-S-Up: perl:font-size:incglobal
URxvt*keysym.C-S-Down: perl:font-size:decglobal

Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault

URxvt.font: xft:Source Code Pro:light:size=10
URxvt.boldFont: xft:Source Code Pro:medium:size=10
URxvt.letterSpace: -2

URxvt*foreground: #bebebe
URxvt*background: #212121
URxvt*colorBD: #ffffff
URxvt*color0: #212121
URxvt*color1: #bf7276
URxvt*color2: #86af80
URxvt*color3: #968a38
URxvt*color4: #3673b5
URxvt*color5: #9a70b2
URxvt*color6: #7abecc
URxvt*color7: #dbdbdb
URxvt*color8: #6692af
URxvt*color9: #e5505f
URxvt*color10: #87bc87
URxvt*color11: #e0d95c
URxvt*color12: #1b85d6
URxvt*color13: #ad73ba
URxvt*color14: #338eaa
URxvt*color15: #f4f4f4
